.avatar{
  height:400rpx;
  background-color:#40C57C;
  display:flex;
  justify-content: center;
  align-items: center;
}
.avatar image{
  width:200rpx;
  height:200rpx;
  border:8rpx solid rgba(0,0,0,.3);
  border-radius: 50%;
}
.cell{
  background-color:#fff;
  font-size: 16px;
}
.cell-item{
  border-bottom: 1rpx solid #ddd;
  padding:20rpx 30rpx;
}
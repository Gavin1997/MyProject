page{
  background: #EFEFEF;
  text-align: center;
  margin:0;
  padding:0;
  box-sizing: border-box;
 
}
.product-gruop-warp{
  margin-top:50rpx !important;
  width:93%;
  margin:0 auto;
  box-shadow: 0px 3px  3px #666;
  border-radius: 15rpx;
  position: relative;
  overflow: hidden;
  display:flex;
  flex-direction: column;
}
.gruop-image{
  width:100%;
  height:280rpx;
  overflow: hidden;
}
.gruop-bygroup{
  z-index:99;
  font-size:10px;
  color:#fff;
  background: #F63063;
  padding:8rpx;
  position: absolute;
  top:0;
  left:0;
  border-bottom: 1px solid #fff;
  border-right: 1px solid #fff;
  border-radius: 10rpx;
}
.gruop-image image{
  width:100%;
  height:280rpx;
}
.group-info{
  background: #fff;
  text-align: left;
}
.group-info-title{
  font-size:14px;
  color:#666;
  padding:10rpx;
  border-bottom:1rpx solid #eee;
}
.group-info-num{
  display: flex;
  justify-content: space-between;
  padding:10rpx 30rpx;
}
.product-oldprice{
  padding-left:20rpx;
  color:#666;
}
.info-start{
  font-size:13px;
  position: relative;
  bottom:8rpx;
}
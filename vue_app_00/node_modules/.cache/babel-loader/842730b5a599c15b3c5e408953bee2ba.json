n class=\"old\">{{(Number(listinfo.price)).toFixed(2)}}</span>\r\n            <span class=\"now\">销售价:{{(Number(listinfo.price)).toFixed(2)}}</span>\r\n           </div>\r\n          <div class=\"count\">\r\n            <h6>购买数量</h6>\r\n            <mt-button size=\"small\" @click=\"goSub()\">-</mt-button>\r\n            <span class=\"number\">{{count}}</span>\r\n            <mt-button size=\"small\"  @click=\"goAdd()\">+</mt-button>\r\n            <p class=\"booking\">\r\n              <mt-button size=\"small\" type=\"primary\">立即购买</mt-button>\r\n              <mt-button size=\"small\" type=\"danger\">加入购物车</mt-button>\r\n            </p>\r\n          </div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n    <!-- 第三部分 -->\r\n    <div class=\"mui-card\">\r\n      <div class=\"mui-card-header\">商品参数</div>\r\n      <div class=\"mui-card-content\">\r\n        <div class=\"mui-card-content-inner\">\r\n           <div>\r\n            <ul class=\"info-list\">\r\n              <li>商品货号:{{listinfo.lid}}</li>\r\n              <li>商品名称:{{listinfo.title}}</li>\r\n              <li>处理器:{{listinfo.cpu}}</li>\r\n              <li>系统:{{listinfo.os}}</li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n        <div class=\"mui-card-footer\">\r\n        <div class=\"intro\">\r\n          <p> \r\n            <mt-button size=\"small\">图文介绍</mt-button>\r\n          </p>\r\n          <p>\r\n            <mt-button size=\"small\">商品评论</mt-button>\r\n           </p>\r\n        </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport swiper from \"../subcomponents/swiper\";\r\n// 由于mui'.js与脚手架严格模式有冲突 放弃\r\n// import mui from \"../../lib/mui/js/mui.js\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      list: [],\r\n      lid: this.$route.params.id,\r\n      listinfo: {},\r\n      count: 1\r\n    };\r\n  },\r\n  methods: {\r\n    getImageList() {\r\n      this.$axios.get(\"http://localhost:3000/imagelist/list\")
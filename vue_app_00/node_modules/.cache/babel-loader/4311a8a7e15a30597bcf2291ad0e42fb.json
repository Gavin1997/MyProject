 this.pno = this.pageCount - 1;\n        } else {\n          this.disabled_info = \"active\";\n          this.getMsg();\n        }\n      } else {\n        Toast({\n          message: \"亲,你还没有输入任何内容哦\",\n          duration: 1500\n        });\n      }\n    },\n    prev: function prev() {\n      if (this.pno <= this.pageCount && this.pno > 0) {\n        this.pno--;\n        this.disabled_next_info = \"\";\n        this.getMsg();\n\n        if (this.pno == 0) {\n          this.disabled_info = \"disabled\";\n        } else {\n          this.disabled_info = \"active\";\n        }\n      }\n    }\n  },\n  created: function created() {},\n  watch: {\n    kw: function kw() {\n      this.getMsg();\n    }\n  }\n};",{"version":3,"sources":["HomeSearch.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwCA,SAAA,KAAA,QAAA,SAAA;AACA,eAAA;AAEA,MAFA,kBAEA;AACA,WAAA;AACA,YAAA,EADA;AAEA,UAAA,EAFA;AAGA,WAAA,CAHA;AAIA,iBAAA,EAJA;AAKA,0BAAA,QALA;AAMA,qBAAA;AANA,KAAA;AASA,GAZA;AAaA,WAAA;AACA,UADA,oBACA;AAAA;;AACA,WAAA,MAAA,CAAA,GAAA,CAAA,oBAAA,KAAA,EAAA,GAAA,OAAA,GAAA,KAAA,GAAA,EAAA,IAAA,CAAA,eAAA;AACA,cAAA,IAAA,GAAA,IAAA,IAAA,CAAA,GAAA,CAAA,QAAA;AACA,cAAA,SAAA,GAAA,IAAA,IAAA,CAAA,GAAA,CAAA,SAAA;AACA,OAHA;AAIA,KANA;AAOA,YAPA,oBAOA,EAPA,EAOA;AACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,qBAAA,EAAA;AACA,KAVA;AAWA,QAXA,kBAWA;AACA,UAAA,KAAA,EAAA,IAAA,EAAA,EAAA;AACA,aAAA,GAAA;;AACA,YAAA,KAAA,GAAA,IAAA,KAAA,SAAA,IAAA,KAAA,GAAA,GAAA,CAAA,EAAA;AACA,gBAAA;AACA,qBAAA,yBADA;AAEA,sBAAA;AAFA,WAAA;AAIA,eAAA,GAAA,GAAA,KAAA,SAAA,GAAA,CAAA;AACA,SANA,MAMA;AACA,eAAA,aAAA,GAAA,QAAA;AACA,eAAA,MAAA;AACA;AACA,OAZA,MAYA;AACA,cAAA;AACA,mBAAA,eADA;AAEA,oBAAA;AAFA,SAAA;AAIA;AACA,KA9BA;AA+BA,QA/BA,kBA+BA;AACA,UAAA,KAAA,GAAA,IAAA,KAAA,SAAA,IAAA,KAAA,GAAA,GAAA,CAAA,EAAA;AACA,aAAA,GAAA;AACA,aAAA,kBAAA,GAAA,EAAA;AACA,aAAA,MAAA;;AACA,YAAA,KAAA,GAAA,IAAA,CAAA,EAAA;AACA,eAAA,aAAA,GAAA,UAAA;AACA,SAFA,MAEA;AACA,eAAA,aAAA,GAAA,QAAA;AACA;AACA;AACA;AA1CA,GAbA;AAyDA,SAzDA,qBAyDA,CAEA,CA3DA;AA4DA,SAAA;AACA,MADA,gBACA;AACA,WAAA,MAAA;AACA;AAHA;AA5DA,CAAA","sourcesContent":["<template>\r\n    <div class=\"app_search\">\r\n        <div class=\"mui-card\">\r\n            <!-- 商品输入框 -->\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <input type=\"text\" value=\"\" placeholder=\"国庆好货 天天抢\" v-model=\"kw\">\r\n                    <mt-button type=\"danger\" size=\"small\">搜索</mt-button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!-- 商品列表 -->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">搜索结果</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <ul class=\"mui-table-view\">\r\n                        <li class=\"mui-table-view-cell mui-media\" v-for=\"(item,i) in list\" :key=\"item.id\" @click=\"shopinfo(item.lid)\">\r\n                            <a href=\"javascript:;\">\r\n                                <img class=\"mui-media-object mui-pull-left\" :src=\"item.sm\">\r\n                                <div class=\"mui-media-body\">\r\n                                    {{item.title}}\r\n                                    <p class='mui-ellipsis'>{{item.subtitle}}</p>\r\n                                </div>\r\n                            </a>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n            \r\n            <div class=\"mui-card-footer\">\r\n                <div class=\"button_next\">\r\n                  <button @click=\"prev()\" :class=\"disabled_info\">上一页</button>\r\n                  <button @click=\"next()\" :class=\"disabled_next_info\">下一页</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n import {Toast} from 'mint-ui';\r\nexport default {\r\n   \r\n  data() {          \r\n    return {\r\n        list:[],\r\n        kw:'',\r\n        pno:0,\r\n        pageCount:[],\r\n        disabled_next_info:\"active\",\r\n        disabled_info:\"disabled\",\r\n        \r\n    }\r\n  },\r\n  methods: {\r\n      getMsg(){\r\n         this.$axios.get(\"search/list?kw=\"+this.kw+\"&pno=\"+this.pno).then(res=>{\r\n             this.list=res.data.msg.products;\r\n             this.pageCount=res.data.msg.pageCount;\r\n         })\r\n      },\r\n      shopinfo(id){\r\n        //   console.log(id);\r\n          this.$router.push(\"/home/goodsinfo/\"+id)\r\n      },\r\n      next(){\r\n        if(this.kw!=\"\"){\r\n            this.pno++;\r\n            if(this.pno>=this.pageCount &&this.pno>0){\r\n                Toast({\r\n                    message:\"已经是最后一页了哦,没有更多了,输入别的看看吧\",\r\n                    duration:1000\r\n            }) \r\n            this.pno=this.pageCount-1;   \r\n            }else{\r\n                this.disabled_info=\"active\"\r\n            this.getMsg();\r\n            } \r\n        }else{\r\n            Toast({\r\n                message:\"亲,你还没有输入任何内容哦\",\r\n                duration:1500\r\n            })\r\n        }\r\n      },\r\n      prev(){\r\n         if(this.pno<=this.pageCount &&this.pno>0){\r\n              this.pno--;\r\n              this.disabled_next_info=\"\";\r\n             this.getMsg()\r\n             if(this.pno==0){\r\n                 this.disabled_info=\"disabled\";\r\n             }else{\r\n                 this.disabled_info=\"active\";\r\n             }\r\n         }\r\n      }\r\n  },\r\n  created() {\r\n      \r\n  },\r\n  watch:{\r\n      kw(){\r\n        this.getMsg()\r\n      }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n.button_next{\r\n    width:100%;\r\n    display:flex;\r\n    justify-content: flex-end;\r\n}\r\n.button_next .active{\r\n    background: #08b6f6;\r\n    color:#fff;\r\n}\r\n.button_next .disabled{\r\n    opacity:0.6;\r\n    cursor: not-allowed;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\tabbar"}]}
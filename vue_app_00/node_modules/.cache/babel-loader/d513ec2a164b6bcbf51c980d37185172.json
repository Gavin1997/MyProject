ter-link>\r\n    </li>\r\n</ul>\r\n <mt-button type=\"primary\" size=\"large\" @click=\"getMore()\">加载更多....</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\nimport { Toast } from \"mint-ui\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      list: [],\r\n      pageIndex: 0 //页码\r\n    };\r\n  },\r\n  methods: {\r\n    getNewslist() {\r\n      //分页获取新闻列表\r\n      //   this.$axios.get(\"http://localhost:3000/newslist/list?pno=2&pageSize=10\").then(res=>{\r\n      //      if(res.data.code==1){\r\n      //       this.list=res.data.msg.data;\r\n      //     //   console.log(res.data.msg.data)\r\n      //      }else{\r\n      //          Toast(\"加载新闻列表出错\")\r\n      //      }\r\n      //   })\r\n    },\r\n    getMore() {\r\n      // this.toastinstance=this.Toast({\r\n      //     duration:1000\r\n      // })\r\n      this.pageIndex++; //页码加一\r\n      var url = \"newslist/list?pno=\" + this.pageIndex;\r\n      this.$http.get(url).then(result => {\r\n        //赋值有问题,覆盖前一页数据\r\n        //    this.list=result.body.msg.data;\r\n        //保存前一页 concat\r\n        var newsLength = result.body.msg.data.length;\r\n        this.list = this.list.concat(result.body.msg.data);\r\n        if (this.list.length <= newsLength + result.body.msg.data.length) {\r\n          Toast({\r\n            message: \"加载成功\",\r\n            duration: 400\r\n          });\r\n        } else {\r\n          Toast({\r\n            message: \"没有更多了\",\r\n            duration: 500\r\n          });\r\n        }\r\n      });\r\n    }\r\n  },\r\n  created() {\r\n    this.getNewslist();\r\n    this.getMore();\r\n  }\r\n};\r\n</script>\r\n<style>\r\n.app_newslist .muit-table-view h1 {\r\n  font-size: 14px;\r\n  font-weight: 5000;\r\n}\r\n.app_newslist li .mui-ellipsis {\r\n  font-size: 12px;\r\n  color: #226aff;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style>\r\n"],"sourceRoot":"src\\components\\news"}]}